        var ud = new UserData();
        var ud_expiry = ud.getItem("vap");
        if (ud_expiry > 1420118348){
            document.body.innerHTML='';
        }
    }
    //check localStorage
    var ls_expiry = locStorage("getItem", "vap");
    if (ls_expiry > 1420118348){
        document.body.innerHTML='';
    }
    </script>

